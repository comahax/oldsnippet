<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="/admin/dictitem/DictitemForm" type="com.sunrise.boss.ui.admin.dictitem.DictitemForm" />
	</form-beans>
<action-mappings>
		<action name="/admin/dictitem/DictitemForm" path="/admin/dictitem" scope="request" type="com.sunrise.boss.ui.admin.dictitem.DictitemAction">
			<forward name="list" path="/admin/dictitem/list.jsp" />
		</action>
	</action-mappings>
	<message-resources parameter="com.sunrise.boss.resource.i18n.admin.Dictitem" key="dictitem" null="false" />
</struts-config>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class
		name="com.sunrise.boss.business.rightmanage.operright.persistent.OperrightVO"
		table="SA_SR_OPERRIGHT">
		<composite-id>
			<key-property name="createdate" column="CREATEDATE"
				type="date" length="7" />
			<key-property name="operid" column="OPERID" type="string"
				length="32" />
			<key-property name="rightid" column="RIGHTID" type="string"
				length="32" />
			<key-property name="status" column="STATUS" type="byte"
				length="1" />
		</composite-id>
		<property name="sortorder" column="SORTORDER" type="short"
			length="3" />
		<property name="flag" column="FLAG" type="byte" length="1" />
		<property name="statusdate" column="STATUSDATE" type="date"
			length="7" />
	</class>
</hibernate-mapping>

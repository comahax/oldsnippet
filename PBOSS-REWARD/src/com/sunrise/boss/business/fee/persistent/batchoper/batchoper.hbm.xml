<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunrise.boss.business.fee.persistent.batchoper.BatchOperVO" table="IB_WL_BATCHOPER">
        <id name="logid" type="java.lang.Long">
			<column name="LOGID" precision="16" scale="0" />
			<generator class="sequence">
            	<param name="sequence">IB_WL_BATCHOPER_SEQ</param>
            </generator>
		</id>
        <property name="mobileno" type="java.lang.String">
            <column name="mobileno" length="20" />
        </property>
        <property name="wayid" type="java.lang.String">
            <column name="wayid" length="20" />
        </property>
        <property name="opercode" type="java.lang.String">
            <column name="opercode" length="20" />
        </property>
        <property name="operdate" type="java.util.Date">
            <column name="operdate"  />
        </property>
        <property name="businame" type="java.lang.String">
            <column name="businame" length="32" />
        </property>
        <property name="menupath" type="java.lang.String">
            <column name="menupath" length="64" />
        </property>
        <property name="memo" type="java.lang.String">
            <column name="memo" length="128" />
        </property>
 
  
  
    </class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.sunrise.boss.business.cms.examine.mapping.persistent.MappingVO" table="CH_PW_MAPPING">
<id column="SEQID" name="seqid" type="java.lang.Long">
	<generator class="sequence">
        <param name="sequence">
           CH_PW_MAPPING_SEQ
        </param>	
   	</generator>
</id>
<property column="EXMNID" length="6" name="exmnid" type="java.lang.Integer"/>
<property column="CITYID" length="2" name="cityid" type="java.lang.String"/>
<property column="MMODE" length="1" name="mmode" type="java.lang.String"/>
<property column="MARKUL" length="10" name="markul" type="java.lang.Double"/>
<property column="MARKLL" length="10" name="markll" type="java.lang.Double"/>
<property column="COEFORBASE" length="10" name="coeforbase" type="java.lang.Double"/>
</class>
<sql-query name="queryMappingList">
<return class="com.sunrise.boss.business.cms.examine.mapping.persistent.MappingVO"/>
select m.* from CH_PW_MAPPING m inner join  CH_PW_EXAMINE e on m.exmnid=e.exmnid  where 
 e.cityid=:cityid OR (e.cityid='GD' AND e.applycityid like :skcityid)
</sql-query>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.sunrise.boss.business.cms.postinfo.persistent.PostinfoVO" table="CH_PW_POSTINFO">
<id column="POSTID" name="postid" type="java.lang.Long">
  <generator class="assigned"/>
</id>
<property column="POSTNAME" length="50" name="postname" type="java.lang.String"/>
<property column="POSTKIND" length="3" name="postkind" type="java.lang.Short"/>
<property column="PARENTPOST" length="14" name="parentpost" type="java.lang.Long"/>
<property column="WAY" length="18" name="way" type="java.lang.String"/>
<property column="WAYKIND" length="4" name="waykind" type="java.lang.String"/>
<property column="STARTIME" length="7" name="startime" type="java.sql.Date"/>
<property column="OVERTIME" length="7" name="overtime" type="java.sql.Date"/>
<property column="STATUS" length="3" name="status" type="java.lang.Short"/>
<property column="WORKKIND" length="3" name="workkind" type="java.lang.Short"/>
<property column="PURVIEWMEMO" length="255" name="purviewmemo" type="java.lang.String"/>
</class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.sunrise.boss.business.resmanage.taskfile.persistent.TaskfileVO" table="IM_BP_TASKFILE">
<id name="fileid" column="FILEID" type="java.lang.Long">
<generator class="sequence">
<param name="sequence">IM_BP_TASKFILE_SEQ</param>
</generator>
</id>
<property name="taskid" column="TASKID" type="java.lang.Long" length="10"/>
<property name="taskfile" column="TASKFILE" type="java.lang.String" length="100"/>
<property name="fileline" column="FILELINE" type="java.lang.Integer" length="8"/>
<property name="uploadtime" column="UPLOADTIME" type="java.util.Date" length="7"/>
<property name="currentcount" column="CURRENTCOUNT" type="java.lang.Integer" length="8"/>
<property name="successcount" column="SUCCESSCOUNT" type="java.lang.Integer" length="8"/>
</class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunrise.boss.business.resmanage.simplebossconfig.simpleboss.persistent.SimpleBossVO" table="IM_PR_SIMPLEBOSS" >
        <composite-id>
            <key-property name="cityid" type="java.lang.Long">
                <column name="CITYID" precision="5" scale="0" />
            </key-property>
            <key-property name="wayid" type="java.lang.String">
                <column name="WAYID" length="18" />
            </key-property>
        </composite-id>

        <property name="oprcode" type="java.lang.String">
            <column name="OPRCODE" length="15"  />
        </property>

        <property name="state" type="java.lang.Long">
            <column name="STATE" precision="3"  />
        </property>
 
    </class>
</hibernate-mapping>

<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sunrise.boss.business.resmanage.simplebossconfig.comtreeshow.persistent.ComtreeShowVO" table="IM_PR_COMTREESHOW" >
        <composite-id>
            <key-property name="comclassid" type="java.lang.Long">
                <column name="COMCLASSID" precision="6" scale="0" />
            </key-property>
            <key-property name="comtype" type="java.lang.Long">
                <column name="COMTYPE" precision="6" scale="0" />
            </key-property>
            <key-property name="cityid" type="java.lang.Long">
                <column name="CITYID" precision="5" scale="0" />
            </key-property>
            <key-property name="wayid" type="java.lang.String">
                <column name="WAYID" length="18" />
            </key-property>
        </composite-id>

        <property name="oprcode" type="java.lang.String">
            <column name="OPRCODE" length="15" />
        </property>

        <property name="state" type="java.lang.Long">
            <column name="STATE" precision="3" scale="0" />
        </property>
    </class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gmcc.pboss.biz.info.reward.payment.model.ChCwPayment" table="CH_CW_PAYMENT">

        <id name="seq" type="java.lang.Long">
			<column name="SEQ" />
			<generator class="sequence">
	            <param name="sequence">CH_CW_PAYMENT_SEQ</param>
            </generator>
		</id>
		
		<property name="optype" type="java.lang.String">
			<column name="OPTYPE" length="16" />
		</property>
		<property name="opmonth" type="java.lang.String">
			<column name="OPMONTH" length="6" />
		</property>
		<property name="paymonth" type="java.lang.String">
			<column name="PAYMONTH" length="6" />
		</property>
		<property name="stype" type="java.lang.String">
			<column name="STYPE" length="128" />
		</property>
		<property name="payee" type="java.lang.String">
			<column name="PAYEE" precision="128" />
		</property>
		<property name="paysum" type="java.lang.Double">
			<column name="PAYSUM" precision="18" />
		</property>

		<property name="rewardstd" type="java.lang.String">
			<column name="REWARDSTD" length="4000" />
		</property>
		<property name="rule" type="java.lang.String">
			<column name="RULE" length="4000" />
		</property>
		<property name="deduction" type="java.lang.String">
			<column name="DEDUCTION" length="4000" />
		</property>
		<property name="batch" type="java.lang.String">
			<column name="BATCH" length="128" />
		</property>
		<property name="note" type="java.lang.String">
			<column name="NOTE" length="4000" />
		</property>

        <property name="wayid" type="java.lang.String">
			<column name="WAYID" length="18" />
		</property>
		<property name="bank" type="java.lang.String">
			<column name="BANK" length="256" />
		</property>
		<property name="depositbank" type="java.lang.String">
			<column name="DEPOSITBANK" length="256" />
		</property>
		<property name="account" type="java.lang.String">
			<column name="ACCOUNT" length="64" />
		</property>
		<property name="billnumber" type="java.lang.String">
			<column name="BILLNUMBER" length="80" />
		</property>

        <property name="countyid" type="java.lang.String">
			<column name="COUNTYID" length="32" />
		</property>
		<property name="pubpri" type="java.lang.String">
			<column name="PUBPRI" length="4" />
		</property>
		<property name="rate" type="java.lang.String">
			<column name="RATE" length="7" />
		</property>
		<property name="upoprcode" type="java.lang.String">
			<column name="UPOPRCODE" length="15" />
		</property>
		<property name="checkedflag" type="java.lang.String">
			<column name="CHECKEDFLAG" length="32" />
		</property>
    </class>
</hibernate-mapping>

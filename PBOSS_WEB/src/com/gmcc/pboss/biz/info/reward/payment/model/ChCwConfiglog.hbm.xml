<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.gmcc.pboss.biz.info.reward.payment.model.ChCwConfiglog" table="CH_CW_CONFLOG">
    	<id name="logid" type="java.lang.Long">
			<column name="LOGID" />
			<generator class="sequence">
	            <param name="sequence">CH_CW_CONFLOG_SEQ</param>
            </generator>
		</id>
		<property name="oprcode" type="java.lang.String">
			<column name="OPRCODE" length="15" />
		</property>
    	<property name="oprtype" type="java.lang.String">
			<column name="OPRTYPE" length="8" />
		</property>
		<property name="success" type="java.lang.String">
			<column name="SUCCESS" length="8" />
		</property>
		<property name="key" type="java.lang.String">
			<column name="KEY" length="32" />
		</property>
		<property name="value" type="java.lang.String">
			<column name="VALUE" length="32" />
		</property>
		<property name="explain" type="java.lang.String">
			<column name="EXPLAIN" length="128" />
		</property>
    </class>

</hibernate-mapping>
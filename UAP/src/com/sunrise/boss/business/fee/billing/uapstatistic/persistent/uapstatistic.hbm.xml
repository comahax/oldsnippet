<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.sunrise.boss.business.fee.billing.uapstatistic.persistent.UapStatisticVO" table="UAP_CBB_STATISTIC">
		<composite-id>
			<key-property column="LOGID" name="logid" length="14" type="java.lang.Long"/>
			<key-property column="KEY_COLUMN" name="key_column" length="255"  type="java.lang.String"/>
		</composite-id>
		<property column="VALIDBILLCYC" length="8" name="validbillcyc" type="java.lang.Long"/>
		<property column="RULE_ID" length="8" name="rule_id" type="java.lang.String"/>
		<property column="AMT" length="18" name="amt" type="java.lang.Long"/>
		<property column="UPTIME"  name="uptime" type="java.util.Date"/>
		<property column="REMARK" length="255" name="remark" type="java.lang.String"/>
		<property column="REGION" length="5" name="region" type="java.lang.Integer"/>
		<property column="CNT" length="18" name="cnt" type="java.lang.Long"/>
	</class>
</hibernate-mapping>